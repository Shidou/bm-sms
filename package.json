{
  "name": "bm-sms",
  "version": "0.2.14",
  "description": "BMQB SMS package",
  "main": "lib/index.js",
  "scripts": {
    "ava": "NODE_ENV=test node --harmony --harmony-proxies ./node_modules/.bin/ava --verbose",
    "test": "./node_modules/.bin/nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "lint": "eslint src",
    "build": "rm -rf ./lib && ./node_modules/.bin/babel src --out-dir lib",
    "prepublish": "npm install --ignore-scripts && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmqb/bm-sms.git"
  },
  "keywords": [
    "bmqb",
    "bm-sms"
  ],
  "author": "xuqian@bmqb.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmqb/bm-sms/issues"
  },
  "homepage": "https://github.com/bmqb/bm-sms#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "nyc": "^8.4.0",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "app-root-path": "^1.3.0",
    "babel-polyfill": "^6.13.0",
    "lodash": "^4.14.2",
    "moment": "^2.14.1",
    "request": "^2.74.0",
    "request-debug": "^0.2.0",
    "request-promise": "^4.1.1",
    "uuid": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "**/*.js"
    ],
    "failFast": false,
    "tap": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  }
}